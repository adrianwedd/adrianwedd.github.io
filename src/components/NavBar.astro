---
const links = [
  { href: '/', label: 'Home' },
  { href: '/tools/', label: 'Tools' },
  { href: '/logs/', label: 'Logs' },
  { href: '/garden/', label: 'Garden' },
];
---
<nav class="bg-gray-800 text-white">
  <div class="container mx-auto px-4 py-2 flex space-x-4">
    {links.map(link => (
      <a href={link.href} class="hover:underline">
        {link.label}
      </a>
    ))}
  </div>
</nav>
